<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.java.j2seproject</type>
    <configuration>
        <buildExtensions xmlns="http://www.netbeans.org/ns/ant-build-extender/1">
            <extension file="build-native.xml" id="j2sedeploy"/>
        </buildExtensions>
        <data xmlns="http://www.netbeans.org/ns/j2se-project/3">
            <name>SPLogic</name>
            <source-roots>
                <root id="src.conf.dir"/>
                <root id="src.amfservices.dir"/>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/2">
            <reference>
                <foreign-project>MinaConnection</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${build.script.reference.MinaConnection}</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>MinaConnection-1</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${project.MinaConnection-1}/build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>SPShare</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${project.SPShare}/build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
        <spellchecker-wordlist xmlns="http://www.netbeans.org/ns/spellchecker-wordlist/1">
            <word>amf</word>
            <word>boxegg</word>
            <word>config</word>
            <word>exp</word>
            <word>ibshop</word>
            <word>Init</word>
            <word>param</word>
            <word>Redis</word>
        </spellchecker-wordlist>
    </configuration>
</project>
